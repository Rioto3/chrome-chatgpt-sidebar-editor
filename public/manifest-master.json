{
  "manifest_version": 3,
  "name": "AIチャットエディタ＋ for マイGPT",
  "version": "1.0.0",
  "description": "エディタの拡張＋マイGPTチャットをプロジェクトに整理",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://chat.openai.com/*",
    "https://chatgpt.com/*",
    "http://ik1-402-33203.vs.sakura.ne.jp/*"
  ],
    "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "sidebar_action": {
    "default_panel": "sidepanel.html",
    "default_title": "AIチャットエディタ＋ for マイGPT",
    "default_icon": {
      "16": "icon-16x16.png",
      "32": "icon-32x32.png",
      "48": "icon-48x48.png",
      "128": "icon-128x128.png"
    }
  },
  "action": {
    "default_title": "AIチャットエディタ＋ for マイGPT",
    "default_icon": {
      "16": "icon-16x16.png"
    }
  },
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icon-16x16.png",
    "32": "icon-32x32.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "aichat-editor-plus@tubeclip.work",
      "strict_min_version": "109.0",
      "data_collection_permissions": {
        "required": [
          "none"
        ],
        "optional": []
      }
    }
  }
}